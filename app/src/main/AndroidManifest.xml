<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yang.yanguitest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.GalleryScrollActivity" />
        <activity android:name=".activity.BezierActivity" />
        <activity android:name=".activity.DragBubbleActivity" />
        <activity android:name=".activity.PathMeasureActivity" />
        <activity android:name=".activity.BehaviorActivity"/>
        <activity android:name=".activity.RecyclerViewActivity"/>
    </application>

</manifest>